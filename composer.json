{
    "name": "strongnguyen/jwt-auth-service",
    "description": "Jwt auth for service",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "StrongNguyen",
            "email": "strongnguyenapp@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "firebase/php-jwt": "^5.4",
        "illuminate/contracts": "^8.0",
        "illuminate/support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "StrongNguyen\\JwtAuthService\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "StrongNguyen\\JwtAuthService\\JwtAuthServiceProvider"
            ],
            "aliases": {
                "JwtAdmin": "StrongNguyen\\JwtAuthService\\Facades\\JwtAdmin",
                "JwtCustomer": "StrongNguyen\\JwtAuthService\\Facades\\JwtCustomer"
            }
        }
    }
}
